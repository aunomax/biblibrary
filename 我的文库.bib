@Article{Fujie2014,
  author           = {Fujie, Kentarou and Winkler, Michael and Yokota, Tomomi},
  title            = {Blow-up prevention by logistic sources in a parabolic–elliptic {Keller}–{Segel} system with singular sensitivity},
  issn             = {0362-546X},
  pages            = {56--71},
  volume           = {109},
  abstract         = {This paper is concerned with the parabolic–elliptic Keller–Segel system with singular sensitivity and logistic source,\{\vphantom{\}}ut=Δu−χ∇⋅(uv∇v)+ru−μu2,x∈Ω,t{\textgreater}0,0=Δv−v+u,x∈Ω,t{\textgreater}0, under homogeneous Neumann boundary conditions in a smoothly bounded domain Ω⊂R2, where χ{\textgreater}0,r∈R,μ{\textgreater}0, with nonnegative initial data 0≢u0∈C0(Ω̄). It is shown that in this two-dimensional setting, the absorptive character of the logistic kinetics is sufficient to enforce global existence of classical solutions even for arbitrarily large χ{\textgreater}0 and any μ{\textgreater}0 and r∈R. It is moreover shown that if in addition r{\textgreater}0 is sufficiently large then all these solutions are uniformly bounded. A main step in the derivation of these results consists of establishing appropriate positive a priori bounds from below for the mass functional ∫Ωu, which due to the presence of logistic kinetics is not preserved. These in turn provide pointwise lower bounds for v, which then allow for the choice of p{\textgreater}1, explicitly depending inter alia on infv, such that ∫Ωup(x,t)dx can be suitably bounded from above.},
  date             = {2014-11},
  doi              = {10.1016/j.na.2014.06.017},
  file             = {ScienceDirect Snapshot:C\:/Users/sass0on/Documents/Zotero/storage/ELNXZL2B/S0362546X14002156.html:text/html;ScienceDirect Full Text PDF:C\:/Users/sass0on/Documents/Zotero/storage/WB4IM9J9/Fujie 等。 - 2014 - Blow-up prevention by logistic sources in a parabo.pdf:application/pdf},
  journaltitle     = {Nonlinear Analysis: Theory, Methods \& Applications},
  keywords         = {Chemotaxis, Logarithmic sensitivity, Boundedness, Global existence},
  language         = {en},
  modificationdate = {2023-09-29T08:56:43},
  url              = {http://www.sciencedirect.com/science/article/pii/S0362546X14002156},
  urldate          = {2020-09-10},
}

@Article{Mizoguchi2014,
  author           = {Mizoguchi, Noriko and Souplet, Philippe},
  title            = {Nondegeneracy of blow-up points for the parabolic {Keller}–{Segel} system},
  issn             = {0294-1449},
  number           = {4},
  pages            = {851--875},
  volume           = {31},
  abstract         = {This paper is concerned with the parabolic Keller–Segel system\{\vphantom{\}}ut=∇⋅(∇u−um∇v)in Ω×(0,T),Γvt=Δv−λv+uin Ω×(0,T), in a domain Ω of RN with N⩾1, where m,Γ{\textgreater}0, λ⩾0 are constants and T{\textgreater}0. When Ω≠RN, we impose the Neumann boundary conditions on the boundary. Under suitable assumptions, we prove the local nondegeneracy of blow-up points. This seems new even for the classical Keller–Segel system (m=1). Lower global blow-up estimates are also obtained. In the singular case 0{\textless}m{\textless}1, as a prerequisite, local existence and regularity properties are established.
Résumé
Dans cet article, nous étudions le système parabolique de Keller–Segel\{\vphantom{\}}ut=∇⋅(∇u−um∇v)dans Ω×(0,T),Γvt=Δv−λv+udans Ω×(0,T), avec Ω un domaine de RN, N⩾1, où m,Γ{\textgreater}0, λ⩾0 sont des constantes et T{\textgreater}0. Lorsque Ω≠RN, les conditions aux limites de Neumann sont prescrites sur le bord. Sous des hypothèses convenables, nous prouvons la non-dégénérescence locale des points d'explosion. Ce résultat semble nouveau même dans le cas du système de Keller–Segel classique (m=1). Des estimations inférieures globales de la vitesse d'explosion sont également obtenues. Dans le cas singulier 0{\textless}m{\textless}1, nous établissons les propriétés nécessaires d'existence locale et de régularité.},
  date             = {2014-07},
  doi              = {10.1016/j.anihpc.2013.07.007},
  file             = {ScienceDirect Snapshot:C\:/Users/sass0on/Documents/Zotero/storage/JCHEYQRB/S0294144913000905.html:text/html;ScienceDirect Full Text PDF:C\:/Users/sass0on/Documents/Zotero/storage/UG8GTAYA/Mizoguchi 和 Souplet - 2014 - Nondegeneracy of blow-up points for the parabolic .pdf:application/pdf},
  journaltitle     = {Annales de l'Institut Henri Poincare (C) Non Linear Analysis},
  keywords         = {Blow-up, Keller–Segel system, Nondegeneracy},
  language         = {en},
  modificationdate = {2023-09-29T08:56:43},
  url              = {http://www.sciencedirect.com/science/article/pii/S0294144913000905},
  urldate          = {2020-09-11},
}

@Article{Alzahrani2019,
  author           = {Alzahrani, Talal and Eftimie, Raluca and Trucu, Dumitru},
  title            = {Multiscale modelling of cancer response to oncolytic viral therapy},
  issn             = {0025-5564},
  pages            = {76--95},
  volume           = {310},
  abstract         = {Oncolytic viruses (OV) are viruses that can replicate selectively within cancer cells and destroy them. While the past few decades have seen significant progress related to the use of these viruses in clinical contexts, the success of oncolytic therapies is dampened by the complex spatial tumour-OV interactions. In this work, we present a novel multiscale moving boundary modelling for the tumour-OV interactions, which is based on coupled systems of partial differential equations both at macro-scale (tissue-scale) and at micro-scale (cell-scale) that are connected through a double feedback link. At the macro-scale, we account for the coupled dynamics of uninfected cancer cells, OV-infected cancer cells, extracellular matrix (ECM) and oncolytic viruses. At the same time, at the micro scale, we focus on essential dynamics of urokinase plasminogen activator (uPA) system which is one of the important proteolytic systems responsible for the degradation of the ECM, with notable influence in cancer invasion. While sourced by the cancer cells that arrive during their macro-dynamics within the outer proliferating rim of the tumour, the uPA micro-dynamics is crucial in determining the movement of the macro-scale tumour boundary (both in terms of direction and displacement magnitude). In this investigation, we consider three scenarios for the macro-scale tumour-OV interactions. While assuming the usual context of reaction-diffusion-taxis coupled PDEs, the three macro-dynamics scenarios gradually explore the influence of the ECM taxis over the tumour - OV interaction, in the form of haptotaxis of both uninfected and infected cells populations as well as the indirect ECM taxis for the oncolytic virus. Finally, the complex tumour-OV interactions is investigated numerically through the development a new multiscale moving boundary computational framework. While further investigation is needed to validate the findings of our modelling, for the parameter regimes that we considered, our numerical simulations indicate that the viral therapy leads to control and decrease of the overall cancer expansion and in certain cases this can result even in the elimination of the tumour.},
  date             = {2019-04},
  doi              = {10.1016/j.mbs.2018.12.018},
  file             = {ScienceDirect Snapshot:C\:/Users/sass0on/Documents/Zotero/storage/2UGL5T5Y/S0025556418302293.html:text/html;ScienceDirect Full Text PDF:C\:/Users/sass0on/Documents/Zotero/storage/FH794VQA/Alzahrani 等。 - 2019 - Multiscale modelling of cancer response to oncolyt.pdf:application/pdf},
  journaltitle     = {Mathematical Biosciences},
  keywords         = {Cancer modelling, Multiscale modelling, Tumour-oncolytic virus interactions},
  language         = {en},
  modificationdate = {2023-09-29T08:56:43},
  url              = {http://www.sciencedirect.com/science/article/pii/S0025556418302293},
  urldate          = {2020-09-17},
}

@Article{Tao2014,
  author           = {Tao, Youshan and Winkler, Michael},
  title            = {Energy-type estimates and global solvability in a two-dimensional chemotaxis–haptotaxis model with remodeling of non-diffusible attractant},
  issn             = {0022-0396},
  number           = {3},
  pages            = {784--815},
  volume           = {257},
  abstract         = {This paper deals with the coupled chemotaxis–haptotaxis model\{\vphantom{\}}ut=Δu−χ∇⋅(u∇v)−ξ∇⋅(u∇w)+μu(1−u−w),x∈Ω,t{\textgreater}0,0=Δv+u−v,x∈Ω,t{\textgreater}0,wt=−vw+ηw(1−w−u),x∈Ω,t{\textgreater}0, which was initially proposed by Chaplain and Lolas (2006) [10] to describe the interactions between cancer cells, the matrix degrading enzyme and the host tissue in a process of cancer cell invasion of tissue (extracellular matrix). Here, Ω⊂R2 is a bounded domain with smooth boundary, and χ, ξ, μ and η are positive parameters. As compared to previous mathematical studies, the novelty here consists of allowing for positive values of η, reflecting processes of self-remodeling of the extracellular matrix. Under zero-flux boundary conditions, it is shown that for any sufficiently smooth initial data (u0,w0) satisfying first-order compatibility conditions, the model admits a unique global smooth solution. A crucial ingredient in the proof is an energy-like inequality which, given T{\textgreater}0, yields boundedness of u(⋅,t) in LlogL(Ω). This serves as a starting point for a bootstrap argument used to derive higher regularity estimates sufficient for global extensibility of solutions.},
  date             = {2014-08},
  doi              = {10.1016/j.jde.2014.04.014},
  file             = {ScienceDirect Snapshot:C\:/Users/sass0on/Documents/Zotero/storage/2GE2H8YQ/S002203961400165X.html:text/html;ScienceDirect Full Text PDF:C\:/Users/sass0on/Documents/Zotero/storage/WARYBAQV/Tao 和 Winkler - 2014 - Energy-type estimates and global solvability in a .pdf:application/pdf},
  journaltitle     = {Journal of Differential Equations},
  keywords         = {Chemotaxis, Energy estimates, Haptotaxis, Logistic proliferation, Tissue remodeling},
  language         = {en},
  modificationdate = {2023-09-29T08:56:43},
  url              = {http://www.sciencedirect.com/science/article/pii/S002203961400165X},
  urldate          = {2020-09-18},
}

