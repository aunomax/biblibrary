	language = {en},
	number = {4},
	urldate = {2020-09-11},
	journal = {Annales de l'Institut Henri Poincare (C) Non Linear Analysis},
	author = {Mizoguchi, Noriko and Souplet, Philippe},
	month = jul,
	year = {2014},
	keywords = {Blow-up, Keller–Segel system, Nondegeneracy},
	pages = {851--875},
	file = {ScienceDirect Snapshot:C\:\\Users\\sass0on\\Documents\\Zotero\\storage\\JCHEYQRB\\S0294144913000905.html:text/html;ScienceDirect Full Text PDF:C\:\\Users\\sass0on\\Documents\\Zotero\\storage\\UG8GTAYA\\Mizoguchi 和 Souplet - 2014 - Nondegeneracy of blow-up points for the parabolic .pdf:application/pdf}
