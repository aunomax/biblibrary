@Article{Salako2016,
  author           = {Salako, Rachidi and Shen, Wenxian},
  title            = {Spreading {Speeds} and {Traveling} waves of a parabolic-elliptic chemotaxis system with logistic source on {R}{\textasciicircum}{N}},
  abstract         = {In this paper, we study the spreading speeds and traveling wave solutions of the PDE \$\$ {\textbackslash}begin\{cases\} u\_\{t\}= {\textbackslash}Delta u-{\textbackslash}chi {\textbackslash}nabla {\textbackslash}cdot (u {\textbackslash}nabla v) + u(1-u),{\textbackslash} {\textbackslash} x{\textbackslash}in{\textbackslash}mathbb\{R\}{\textasciicircum}N 0={\textbackslash}Delta v-v+u, {\textbackslash} {\textbackslash} x{\textbackslash}in{\textbackslash}mathbb\{R\}{\textasciicircum}N, {\textbackslash}end\{cases\} \$\$ where \$u(x,t)\$ and \$v(x,t)\$ represent the population and the chemoattractant densities, respectively, and \${\textbackslash}chi\$ is the chemotaxis sensitivity. It has been shown in an earlier work by the authors of the current paper that, when \$0{\textless}{\textbackslash}chi{\textless}1\$, for every nonnegative uniformly continuous and bounded function \$u\_0(x)\$, the system has a unique globally bounded classical solution \$(u(x,t;u\_0),v(x,t;u\_0))\$ with initial condition \$u(x,0;u\_0)=u\_0(x)\$. Furthermore, if \$0{\textless}{\textbackslash}chi{\textless}{\textbackslash}frac\{1\}\{2\}\$, then the constant steady-state solution \$(1,1)\$ is asymptotically stable with respect to strictly positive perturbations. In the current paper, we show that if \$0{\textless}{\textbackslash}chi{\textless}1\$, then there are nonnegative constants \$c\_\{-\}{\textasciicircum}*({\textbackslash}chi){\textbackslash}leq c\_+{\textasciicircum}*({\textbackslash}chi)\$ such that for every nonnegative initial function \$u\_0({\textbackslash}cdot)\$ with nonempty compact support \$\$ {\textbackslash}lim\_\{t{\textbackslash}to{\textbackslash}infty\} {\textbackslash}sup\_\{{\textbar}x{\textbar}{\textbackslash}le ct\} {\textbackslash}big[{\textbar}u(x,t;u\_0)-1{\textbar}+{\textbar}v(x,t;u\_0)-1{\textbar}{\textbackslash}big]=0{\textbackslash} {\textbackslash} {\textbackslash}forall{\textbackslash} 0{\textless}c{\textless}c\_\{-\}{\textasciicircum}*({\textbackslash}chi) \$\$ and \$\$ {\textbackslash}lim\_\{t{\textbackslash}to{\textbackslash}infty\}{\textbackslash}sup\_\{{\textbar}x{\textbar}{\textbackslash}geq ct\} {\textbackslash}big[ u(x,t;u\_0)+v(x,t;u\_0){\textbackslash}big]=0{\textbackslash} {\textbackslash} {\textbackslash}forall{\textbackslash} c{\textgreater}c\_\{+\}{\textasciicircum}*({\textbackslash}chi).\$\$ We also show that if \$0{\textless}{\textbackslash}chi{\textless}{\textbackslash}frac\{1\}\{2\}\$, there is a positive constant \$c{\textasciicircum}*({\textbackslash}chi)\$ such that for every \$c{\textbackslash}ge c{\textasciicircum}*({\textbackslash}chi)\$, the system has a traveling wave solution \$(u(x,t),v(x,t))\$ with speed \$c\$ and connecting \$(1,1)\$ and \$(0,0)\$, that is, \$(u(x,t),v(x,t))=(U(x-ct),V(x-ct))\$ for some functions \$U({\textbackslash}cdot)\$ and \$V({\textbackslash}cdot)\$ satisfying \$(U(-{\textbackslash}infty),V(-{\textbackslash}infty))=(1,1)\$ and \$(U({\textbackslash}infty),V({\textbackslash}infty))=(0,0)\$. Moreover, \$\$ {\textbackslash}lim\_\{{\textbackslash}chi{\textbackslash}to 0\}c{\textasciicircum}*({\textbackslash}chi)={\textbackslash}lim\_\{{\textbackslash}chi{\textbackslash}to 0\}c\_+{\textasciicircum}*({\textbackslash}chi)={\textbackslash}lim\_\{{\textbackslash}chi{\textbackslash}to 0\}c\_-{\textasciicircum}*({\textbackslash}chi)=2. \$\$ We first give a detailed study in the case \$N=1\$ and next we extend these results to the case \$N{\textbackslash}ge 2\$.},
  date             = {2016-11},
