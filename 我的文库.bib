  date             = {2009-01},
  doi              = {10.1137/090751542},
  file             = {Full Text PDF:C\:/Users/sass0on/Documents/Zotero/storage/SGTSJ6TM/Tao 和 Wang - 2009 - A Combined Chemotaxis-haptotaxis System The Role .pdf:application/pdf;Snapshot:C\:/Users/sass0on/Documents/Zotero/storage/6NQZC3NA/090751542.html:text/html},
  journaltitle     = {SIAM Journal on Mathematical Analysis},
  modificationdate = {2023-09-29T08:56:43},
  shorttitle       = {A {Combined} {Chemotaxis}-haptotaxis {System}},
  url              = {https://epubs.siam.org/doi/10.1137/090751542},
