	journal = {Nonlinearity},
	author = {Jüngel, Ansgar},
	month = may,
	year = {2015},
	keywords = {entropy},
	pages = {1963--2001},
	file = {IOP Full Text PDF:C\:\\Users\\sass0on\\Documents\\Zotero\\storage\\EQD7594S\\Jüngel - 2015 - The boundedness-by-entropy method for cross-diffus.pdf:application/pdf}
