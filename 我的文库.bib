@article{winkler_singular_2018,
	title = {Singular structure formation in a degenerate haptotaxis model involving myopic diffusion},
	volume = {112},
	issn = {0021-7824},
	url = {http://www.sciencedirect.com/science/article/pii/S0021782417301782},
	doi = {10.1016/j.matpur.2017.11.002},
	abstract = {We consider the system(0.1)\{\vphantom{\}}ut=(d(x)u)xx−(d(x)uwx)x,wt=−ug(w), which arises as a simple model for haptotactic migration in heterogeneous environments, such as typically occurring in the invasive dynamics of glioma. A particular focus is on situations when the diffusion herein is degenerate in the sense that the zero set of d is not empty. It is shown that if such possibly present degeneracies are sufficiently mild in the sense that(0.2)∫Ω1d{\textless}∞, then under appropriate assumptions on the initial data a corresponding initial-boundary value problem for (0.1), posed under no-flux boundary conditions in a bounded open interval Ω⊂R, possesses at least one globally defined generalized solution. Moreover, despite such degeneracies the myopic diffusion mechanism in (0.1) is seen to asymptotically determine the solution behavior in the sense that for some constant μ∞{\textgreater}0, the obtained solution satisfies(0.3)u(⋅,t)⇀μ∞din L1(Ω)andw(⋅,t)→0in L∞(Ω)as t→∞, and that hence in the degenerate case the solution component u stabilizes toward a state involving infinite densities, which is in good accordance with experimentally observed phenomena of cell aggregation. Finally, under slightly stronger hypotheses inter alia requiring that 1d belong to Llog⁡L(Ω), a substantial effect of diffusion is shown to appear already immediately by proving that for a.e. t{\textgreater}0, the quantity ln⁡(du(⋅,t)) is bounded in Ω. In degenerate situations, this particularly implies that the blow-up phenomena expressed in (0.3) in fact occur instantaneously.
Résumé
Nous consiérons le système(0.1)\{\vphantom{\}}ut=(d(x)u)xx−(d(x)uwx)x,wt=−ug(w), qui est issu d'un modèle simplifié pour la migration hapto-tactique dans des milieux hétérogènes, comme par exemple dans la dynamique invasive des gliomes. Un intérêt particulier est porté aux situations pour lesquelles la diffusion est dégénérée, c'est-à-dire que le noyau de d est non vide. À (0.1) on peut faire correspondre un problème aux limites, posé sur intervalle ouvert et borné Ω⊂R, associé à une condition de flux nul au bord. Il est montré que si les éventuelles dégénérescences de d sont suffisament modérées au sens que(0.2)∫Ω1d{\textless}∞, alors il existe au moins une solution généralisée au problème, définie globalement. Ce, sous des hypothèses appropriées sur la donnée initiale. De plus, malgré ces dégénérescences, le mécanisme de diffusion myopique de (0.1) determine le comportement asymptotique de la solution. Plus précisément, pour une certaine constante μ∞{\textgreater}0, la solution obtenue satisfait(0.3)u(⋅,t)⇀μ∞ddans L1(Ω)etw(⋅,t)→0dans L∞(Ω)pour t→∞. Donc dans le cas dégénéré, la composante u de la solution converge vers un état présentant des densités infinies. Ceci est en adéquation avec les phénomènes d'aggrégation cellulaire observés expérimentalement. Finalement, sous des hypothèses un peu plus fortes, dont la contrainte d∈Llog⁡L(Ω), il est montré que la diffusion a un effet presque immédiat. C'est-à-dire que pour presque tout t{\textgreater}0, la quantité ln⁡(du(⋅,t)) est bornée dans Ω. Dans le cas dégénéré, ceci implique en particulier que les phénomènes d'explosion décrits par (0.3) ont lieu instantanément.},
	language = {en},
	urldate = {2020-10-22},
	journal = {Journal de Mathématiques Pures et Appliquées},
	author = {Winkler, Michael},
	month = apr,
	year = {2018},
	keywords = {Degenerate diffusion, Blow-up, Haptotaxis, Large time behavior},
	pages = {118--169},
	file = {ScienceDirect Snapshot:C\:\\Users\\sass0on\\Documents\\Zotero\\storage\\KI35B4FP\\S0021782417301782.html:text/html;已提交版本:C\:\\Users\\sass0on\\Documents\\Zotero\\storage\\H3JEFP6V\\Winkler - 2018 - Singular structure formation in a degenerate hapto.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\sass0on\\Documents\\Zotero\\storage\\FG394LQN\\S0021782417301782.html:text/html}
